{
  "name": "coringa-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/http/main/main.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate": "npx ts-node ./node_modules/typeorm/cli.js migration:run ",
    "revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "create": "ts-node ./node_modules/typeorm/cli.js migration:create -n ",
    "gerar": "ts-node ./node_modules/typeorm/cli.js migration:generate -n ",
    "formatter": "npx prettier 'src/**/*.{js,ts,mjs,cjs,json}' --write",
    "check": "npx prettier . --check"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-error": "^0.0.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.1",
    "logger": "^0.0.1",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "0.2.4",
    "typescript": "^5.1.6",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.1",
    "prettier": "3.0.3",
    "ts-loader": "^9.4.4",
    "tsconfig-paths": "^4.2.0"
  }
}
